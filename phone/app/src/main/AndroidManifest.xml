<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yanxiu.gphone.student">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />

    <application
        android:name=".YanxiuApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <!--android:name=".WelcomeActivity"-->
        <activity

            android:name=".homepage.MainActivity"
            android:configChanges="keyboardHidden|orientation"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".user.view.ui.LoginActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppFullscreen"
            android:configChanges="keyboardHidden|orientation"
            android:windowSoftInputMode="stateHidden|adjustPan">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>

        <activity
            android:name=".user.view.ui.ForgetPassWordActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppFullscreen"
            android:configChanges="keyboardHidden|orientation"
            android:windowSoftInputMode="stateHidden|adjustPan" />

        <activity
            android:name=".user.view.ui.RegisterActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppFullscreen"
            android:configChanges="keyboardHidden|orientation"
            android:windowSoftInputMode="stateHidden|adjustPan" />

        <activity android:name=".user.view.ui.ResetPassWordActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppFullscreen"
            android:configChanges="keyboardHidden|orientation"
            android:windowSoftInputMode="stateHidden|adjustPan"/>
        <!--<activity-->
        <!--android:name=".MainActivity"-->
        <!--android:launchMode="singleTask"-->
        <!--android:screenOrientation="portrait">-->
        <!--<intent-filter>-->
        <!--<action android:name="android.intent.action.VIEW" />-->

        <!--<category android:name="android.intent.category.DEFAULT" />-->
        <!--</intent-filter>-->
        <!--</activity>-->
    </application>

</manifest>